<%- include('partials/signheader') -%>

 <div class="signupbox">
    <form method="post" action="/">

      <div class="form-group">
        <label>Name</label>
        <input type="text" placeholder="Enter Name" required autocomplete="off" spellcheck="false" name="name" class="form-control" >
      </div>

      <div class="form-group">
        <label>Password</label>
        <input type="password" placeholder="Enter Password" required autocomplete="off" spellcheck="false" name="password" class="form-control" >
      </div>
         
      <button type="submit" class="btn btnn">Sign-up</button>
          
    </form>
 </div>

<%- include('partials/footer') -%>